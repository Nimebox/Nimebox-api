{
  "name": "nimebox-api",
  "version": "0.3.0",
  "private": true,
  "description": "Nimebox RESTful API",
  "main": "./dist/index",
  "scripts": {
    "dev": "now dev",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint -p tsconfig.json --fix",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand"
  },
  "author": "xdk78 <xdk78888@gmail.com>",
  "contributors": [
    "pcpl2"
  ],
  "license": "MIT",
  "dependencies": {
    "@3846masa/axios-cookiejar-support": "^0.4.2",
    "axios": "^0.18.0",
    "canvas": "file:./src/utils/canvas",
    "cross-env": "^5.2.0",
    "grabbi": "^0.1.4",
    "jsdom": "^15.1.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.1.0",
    "koa-logger": "^3.2.0",
    "lodash": "^4.17.11",
    "q": "^1.5.1",
    "qs": "^6.7.0",
    "tough-cookie": "^3.0.1",
    "x-ray": "^2.3.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/jsdom": "^12.2.3",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-logger": "^3.1.1",
    "@types/lodash": "^4.14.129",
    "@types/node": "^12.0.2",
    "@types/qs": "^6.5.3",
    "dotenv-safe": "^6.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "shx": "^0.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
