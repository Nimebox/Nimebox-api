{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "max-line-length": false,
    "variable-name": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "object-literal-key-quotes": false,
    "quotemark": [
      true,
      "single"
    ]
  }
}